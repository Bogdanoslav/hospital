<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="07" author="bogdanoslav">
        <addNotNullConstraint tableName="working_hours" columnName="start_time" columnDataType="TIME"/>
        <addNotNullConstraint tableName="working_hours" columnName="end_time" columnDataType="TIME"/>
